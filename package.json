{
  "name": "super-awesome-theme",
  "version": "1.0.0",
  "description": "Super Awesome Theme theme for WordPress.",
  "keywords": [
    "wordpress",
    "theme"
  ],
  "author": {
    "name": "Taco Themes",
    "email": "hello@taco-themes.com",
    "url": "https://www.taco-themes.com"
  },
  "contributors": [
    {
      "name": "Felix Arntz",
      "email": "felix-arntz@leaves-and-love.net",
      "url": "https://leaves-and-love.net"
    },
    {
      "name": "Phillip Roth",
      "email": "phillip@philliproth.de",
      "url": "https://www.philliproth.de/"
    }
  ],
  "homepage": "https://www.taco-themes.com/themes/super-awesome-theme/",
  "repository": {
    "type": "git",
    "url": "git://github.com/taco-themes/super-awesome-theme.git"
  },
  "bugs": {
    "url": "https://github.com/taco-themes/super-awesome-theme/issues"
  },
  "license": "GPL-3.0",
  "browserslist": [
    "last 2 versions",
    "android 4",
    "opera 12"
  ],
  "scripts": {
    "build": "webpack --progress --config assets/dev/webpack.config.js",
    "build:production": "webpack --progress -p --config assets/dev/webpack.config.js",
    "build:profile": "webpack --progress --profile --json --config assets/dev/webpack.config.js",
    "start": "webpack --hide-modules --watch --config assets/dev/webpack.config.js",
    "rmdist": "rimraf dist",
    "lint": "npm run -s lint:scripts && npm run -s lint:styles",
    "lint:scripts": "eslint assets/dev",
    "lint:styles": "stylelint \"assets/dev/**/*.{css,sass,scss,sss,less}\"",
    "test": "npm run -s lint"
  },
  "engines": {
    "node": ">= 6.9.4"
  },
  "devDependencies": {
    "autoprefixer": "~7.1",
    "browser-sync": "~2.18",
    "browsersync-webpack-plugin": "^0.6.0",
    "bs-html-injector": "~3.0",
    "buble-loader": "^0.4.1",
    "cache-loader": "~1.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-globs-webpack-plugin": "^0.2.0",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.4",
    "eslint": "~4.2",
    "eslint-loader": "~1.9",
    "eslint-plugin-import": "~2.7",
    "extract-text-webpack-plugin": "~3.0",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "imagemin-mozjpeg": "~6.0",
    "imagemin-webpack-plugin": "~1.5.0-beta.0",
    "import-glob": "~1.5",
    "node-sass": "~4.5",
    "postcss-loader": "~2.0",
    "postcss-csscomb": "~1.0",
    "postcss-safe-parser": "~3.0",
    "rimraf": "~2.6",
    "sass-loader": "~6.0",
    "style-loader": "^0.18.2",
    "stylelint": "~7.12",
    "stylelint-config-standard": "~16.0",
    "stylelint-webpack-plugin": "^0.8.0",
    "uglifyjs-webpack-plugin": "~1.0.0-beta.2",
    "unminified-webpack-plugin": "~1.3",
    "webpack": "~3.3",
    "webpack-dev-middleware": "~1.11",
    "webpack-hot-middleware": "~2.18",
    "webpack-merge": "~4.1",
    "yargs": "~8.0"
  }
}
